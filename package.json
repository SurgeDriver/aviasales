{
  "name": "movie-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --config .eslintrc.mjs",
    "lint:fix": "eslint . --fix --config .eslintrc.mjs",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "antd": "^5.24.1",
    "axios": "^1.8.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-lottie": "^1.2.10",
    "react-redux": "^9.2.0",
    "react-spinners": "^0.15.0",
    "react-window": "^1.8.11",
    "redux-thunk": "^3.1.0",
    "sass": "^1.85.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "less": "^4.2.2",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "sass-embedded": "^1.85.1",
    "stylus": "^0.64.0",
    "vite": "^6.0.5"
  }
}
